/**
 * Return 1 if the highest bit is 1, indicating a negative
 * number in 2's complement
 */

CHIP IsNeg {
    IN in[16];
    OUT out;

    PARTS:
    // Put your code here:
    And(a=in[15], b=true, out=out);
}